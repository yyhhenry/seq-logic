{
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Seq Logic",
    "version": "1.1.2"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "all": true,
        "scope": ["$RESOURCE/samples/*", "$RESOURCE/units/*"]
      },
      "path": {
        "all": true
      },
      "dialog": {
        "all": true
      },
      "clipboard": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.yyhhenry.seq-logic",
      "resources": ["samples/*", "units/*"],
      "targets": ["msi"],
      "windows": {
        "wix": {
          "language": ["zh-CN"]
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Seq Logic",
        "height": 600,
        "width": 800,
        "minHeight": 600,
        "minWidth": 800
      }
    ]
  }
}
